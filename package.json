{
  "name": "symbol-by-ctags",
  "version": "0.1.0",
  "description": "utilize symbols made with ctags",
  "author": "PrsPrsBK",
  "license": "MIT",
  "repository": "https://github.com/PrsPrsBK/vs-symbol-by-ctags",
  "devDependencies": {
    "@types/node": "^11.13.6",
    "vscode": "^1.1.33"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.symbolByCtags"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.symbolByCtags",
        "title": "Symbol by Ctags"
      }
    ],
    "configuration": {
      "title": "Symbol by Ctags",
      "properties": {
        "SymbolByCtags.tags": {
          "type": "array",
          "items": "string",
          "markdownDescription": "An array of tags file name."
        },
        "SymbolByCtags.target": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "name": { "type": "string" },
                "glob": { "type": "string" },
                "exec": { "type": "string" },
                "tags": { "type": "string" }
              }
            }
          ],
          "markdownDescription": "An array of file types to be targeted."
        }
      }
    }
  },
  "engines": {
    "vscode": "^1.33.0"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  }
}